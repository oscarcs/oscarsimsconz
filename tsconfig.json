{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "strict": true,
        "lib": [
            "ESNext",
            "DOM",
            "DOM.Iterable"
        ],
        "types": [
            "@cloudflare/workers-types",
            "vite/client"
        ],
        "jsx": "react-jsx",
        "jsxImportSource": "react",
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./src/*"
            ],
            "three/webgpu": [
                "./node_modules/@types/three/src/Three.WebGPU.d.ts"
            ],
            "three/tsl": [
                "./node_modules/@types/three/src/Three.TSL.d.ts"
            ]
        }
    }
}